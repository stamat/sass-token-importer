{
  "typography": {
    "$type": "typography",
    "heading": {
      "h1": {
        "$value": {
          "fontFamily": ["Helvetica", "Arial", "sans-serif"],
          "fontSize": "32px",
          "fontWeight": 700,
          "lineHeight": 1.2
        }
      },
      "h2": {
        "$value": {
          "fontFamily": ["Helvetica", "Arial", "sans-serif"],
          "fontSize": "24px",
          "fontWeight": 700,
          "lineHeight": 1.3
        }
      }
    },
    "body": {
      "$value": {
        "fontFamily": ["Georgia", "serif"],
        "fontSize": "16px",
        "fontWeight": 400,
        "lineHeight": 1.5
      }
    }
  }
}
